<!DOCTYPE html>
<html>
<head>
  <title>Task 4</title>
</head>
<body>
  <button onclick="handleClick()">Click me</button>

  <script>
    function handleClick() {
      let numbers = [];
      for (let i = 0; i <= 100; i++) {
        numbers.push(i);
      }

      const sum = numbers.reduce((acc, num) => acc + num, 0);
      console.log("Sum of numbers from 0 to 100:", sum);
    }
  </script>
</body>
</html>
